{
  "name": "check-appointments-frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:ci": "ng build -c=$BUILD_CONFIG --baseHref=/check-appointments-frontend-angular/",
